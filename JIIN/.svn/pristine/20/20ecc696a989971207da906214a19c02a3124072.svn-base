<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <ExpandableListView
        android:id="@+id/expandableListView1"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:groupIndicator="@null" >

    </ExpandableListView>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="아래 내용을 함께 보내주시면 더울 빨리 처리가 가능합니다." />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="문제발생일시" />

        <EditText
            android:id="@+id/edit_problem_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10" >

        </EditText>

    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="문의내용" />

        <EditText
            android:id="@+id/edit_problem_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10" >

        </EditText>

    </LinearLayout>
    
            <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <EditText
            android:id="@+id/edit_problem_photo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10" >

        </EditText>

    </LinearLayout>

    <Button
        android:id="@+id/btn_problem_photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사진" />

    <EditText
        android:id="@+id/edit_emailp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="답볌ㄴ 받을 이메일 주소"
        android:ems="10" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="문의하신 내용에 대한 원인 파악 및 원활한 상담을 위하여 이메일 주소를 수집합니다. 수집된 개인정보는 개인정보의 수집 및 이용..." />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <CheckBox
            android:id="@+id/check_agree"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="위 내용에 동의합니다." />


    </LinearLayout>

    <Button
        android:id="@+id/btn_sendproblem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="등록하기" />
    
</LinearLayout>
