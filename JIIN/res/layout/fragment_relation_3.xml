<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/FrameLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="25dp"
    android:layout_marginTop="-5dp" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="00님과 나의 관계는?"
        android:visibility="invisible" />
    
    <FrameLayout
        android:id="@+id/layout_bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:visibility="invisible" >
    
	    <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="120dp"
	        android:src="@drawable/line_right_4" />
	    
	    <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="120dp"
	        android:layout_marginTop="107dp"
	        android:src="@drawable/line_right_4" />
	    
	    <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="120dp"
	        android:layout_marginTop="47dp"
	        android:src="@drawable/line_left_4" />
	    
	</FrameLayout>

    <FrameLayout
        android:id="@+id/layout_me"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="66dp"
        android:layout_marginTop="40dp"
        android:visibility="invisible" >

        <ImageView
            android:id="@+id/image_me_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/ic_me_out_4" />

        <com.jiin.MLRoundedImageView
            android:id="@+id/image_me"
            android:layout_width="@dimen/relation"
            android:layout_height="@dimen/relation"
            android:layout_gravity="center"
            android:layout_marginBottom="1dp"
            android:src="@drawable/ic_in" />

    </FrameLayout>
    
    <com.jiin.MLRoundedImageView
        android:id="@+id/image_friend1"
        android:layout_width="@dimen/relation"
        android:layout_height="@dimen/relation"
        android:layout_marginLeft="171dp"
        android:layout_marginTop="110dp"
        android:src="@drawable/bt_other_4"
        android:visibility="invisible" />
    
	<com.jiin.MLRoundedImageView
	    android:id="@+id/image_friend2"
	    android:layout_width="@dimen/relation"
	    android:layout_height="@dimen/relation"
	    android:layout_marginLeft="76dp"
	    android:layout_marginTop="160dp"
	    android:src="@drawable/bt_other_4"
	    android:visibility="invisible" />

    <FrameLayout
        android:id="@+id/layout_you"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="161dp"
        android:layout_marginTop="210dp"
        android:visibility="invisible" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/ic_you_out" />

        <com.jiin.MLRoundedImageView
            android:id="@+id/image_you"
            android:layout_width="@dimen/relation"
            android:layout_height="@dimen/relation"
            android:layout_gravity="center"
            android:layout_marginBottom="1dp"
            android:src="@drawable/ic_in" />

    </FrameLayout>



</FrameLayout>
